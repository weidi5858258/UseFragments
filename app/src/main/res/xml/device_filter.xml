    <!-- 还要说明一点的是：Windows设备属性中看到的ID值是16进制表示的，必须转换成10进制，并配置 。-->
    <!-- 测试用的亿捷U盘，Windows中显示的VID是090C，转换为十进制是2316，其他的道理是一样的 -->
    <usb-device product-id="4096" vendor-id="2316"/>
    <!-- 测试用的M1读卡器 -->
    <usb-device product-id="22352" vendor-id="1155"/>
    <!-- USB-HUB -->
    <usb-device product-id="1544" vendor-id="1507"/>
